{
    "name": "eugmerkeleon/freshdesk-laravel",
    "description": "Laravel package for the Freshdesk API (v2)",
    "homepage": "https://github.com/Merkeleon/freshdesk-laravel",
    "keywords": ["freshdesk", "customer service", "support", "help desk", "api", "laravel"],
    "type": "laravel-package",
    "require": {
        "mpclarkson/freshdesk-php-sdk": "0.3.*",
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "laravel/laravel": ">=5.0",
        "laravel/lumen": "5"
    },
    "minimum-stability": "dev",
    "autoload" : {
        "psr-4": {
            "Merkeleon\\Laravel\\Freshdesk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Merkeleon\\Laravel\\Freshdesk\\Test\\": "tests/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1-dev"
        }
    }
}
